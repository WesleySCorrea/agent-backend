package com.dev.agent.services.impl;

import com.dev.agent.entity.Pdv;
import lombok.RequiredArgsConstructor;
import com.dev.agent.services.PdvService;
import org.springframework.stereotype.Service;
import com.dev.agent.repository.PdvRepository;
import com.dev.agent.dto.pdv.request.PdvRequestDTO;
import com.dev.agent.dto.pdv.response.PdvResponseDTO;

@Service
@RequiredArgsConstructor
public class PdvServiceImpl implements PdvService {

    private final PdvRepository pdvRepository;

    @Override
    public PdvResponseDTO create(PdvRequestDTO request) {

        Pdv pdv = pdvRepository.save(request.toEntity());
        return null;
    }
}
